                           <font>
                              <Font name="Tahoma Bold" size="12.0" />
                           </font>
                        </CheckBox>
                        <CheckBox fx:id="checkBox5" layoutX="50.0" layoutY="402.0" mnemonicParsing="false" text="5) Connect the Black Jumper Cable to the Negative Terminal">
                           <font>
                              <Font name="Tahoma Bold" size="12.0" />
                           </font>
                        </CheckBox>
                        <CheckBox fx:id="checkBox6" layoutX="165.0" layoutY="427.0" mnemonicParsing="false" text="6) Start the Engine">
                           <font>
                              <Font name="Tahoma Bold" size="12.0" />
                           </font>
                        </CheckBox>
                        <CheckBox fx:id="checkBox7" layoutX="175.0" layoutY="450.0" mnemonicParsing="false" text="7) Wait a Moment">
                           <font>
                              <Font name="Tahoma Bold" size="12.0" />
                           </font>
                        </CheckBox>
                        <CheckBox fx:id="checkBox8" layoutX="164.0" layoutY="473.0" mnemonicParsing="false" text="8) Let the Engine Run">
                           <font>
                              <Font name="Tahoma Bold" size="12.0" />
                           </font>
                        </CheckBox>
                        <CheckBox fx:id="checkBox9" layoutX="133.0" layoutY="495.0" mnemonicParsing="false" text="9) Disconnect the Jumper Cables">
                           <font>
                              <Font name="Tahoma Bold" size="12.0" />
                           </font>
                        </CheckBox>
                        <CheckBox fx:id="checkBox10" layoutX="180.0" layoutY="519.0" mnemonicParsing="false" text="10) Test Drive">
                           <font>
                              <Font name="Tahoma Bold" size="12.0" />
                           </font>
                        </CheckBox>
                        <Text layoutX="32.0" layoutY="297.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Make Sure You Follow Checklist Below And Tick After Done Each Part:" textAlignment="CENTER">
                           <font>
                              <Font name="Tahoma Bold" size="12.0" />
                           </font>
                        </Text>
                     </children>
                  </AnchorPane>
               </content>
